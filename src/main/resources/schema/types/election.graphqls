type Election {
    electionId: Int!
    timeZone:String!
    electionName: String!
    description: String
    startDateTime: DateTime
    endDateTime: DateTime
    status: ElectionStatus!
    isAnonymous: Boolean!
    createdDateTime: DateTime
    updatedDateTime: DateTime
    createdBy: Int!
    registeredNumberOfVotes: Int
    eligibleVoters: [String]
    candidates: [Candidate]
}

enum ElectionStatus {
    UPCOMING,
    ONGOING,
    COMPLETED,
    CANCELLED,
}